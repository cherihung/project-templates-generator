{
  "name": "benchmark-cli",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "start-watch": "tsc-watch -p src/tsconfig.json --onSuccess 'yarn start'",
    "build": "yarn clean && tsc --project src/tsconfig.json && yarn copy:templates",
    "clean": "rm -rf dist",
    "copy:templates": "cp -r templates dist"
  },
  "bin": {
    "generate": "./dist/index.js"
 },
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.15",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "inquirer": "^8.2.0"
  }
}
